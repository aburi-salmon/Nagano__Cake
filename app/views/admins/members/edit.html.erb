<h2>会員情報編集</h2>
会員ID:<%= @member.id %>
    <%= form_with model:[:admins, @member],method: :patch, local: true do |f| %>
    氏名<%= f.text_field :last_name %> <%= f.text_field :first_name %><br>
    フリガナ<%= f.text_field :kana_last_name %> <%= f.text_field :kana_first_name %><br>
    郵便番号<%= f.text_field :postal_code %><br>
    住所<%= f.text_field :address %><br>
    電話番号<%= f.text_field :phone_number %><br>
    メールアドレス<%= f.email_field :email %><br>
    
    会員ステータス:
    <%= f.radio_button :is_deleted,false %>
    <%= f.label :is_deleted, '有効', value: false %>
    <%= f.radio_button :is_deleted,true %>
    <%= f.label :is_deleted, '退会', value: true %><br>
    
    <%= f.submit "変更を保存" %>

<% end %>