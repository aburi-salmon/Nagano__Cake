<h2>ジャンル編集</h2>
<div class="container">
  <div class="row">
    <div class="col">
      <%= form_with model: [:admin, @genre],method: :patch, local: true do |f| %>
        <%= "ジャンル名" %>
        <%= f.text_field :name %>
        <%= f.submit "変更を保存", class: 'btn btn-success' %>
      <% end %>
    </div>
  </div>
</div>